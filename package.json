{
  "name": "hono-compress",
  "type": "module",
  "version": "1.0.0",
  "description": "Compression plugin for Hono working with Bun",
  "author": {
    "name": "Walter Purcaro",
    "url": "https://github.com/vuolter"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuolter/hono-compress.git"
  },
  "keywords": [
    "hono",
    "hono compress",
    "bun compress",
    "compression",
    "compress",
    "node",
    "deno",
    "bun",
    "edge",
    "zstd",
    "brotli",
    "gzip",
    "deflate"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "url": "https://github.com/vuolter/hono-compress",
  "scripts": {
    "build": "bun typecheck && tsdown",
    "format": "prettier . --cache --experimental-cli --write",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --cache-strategy content --concurrency auto --fix .",
    "typecheck": "tsgo",
    "gendocs": "typedoc && prettier ./docs --cache --experimental-cli --log-level silent --write",
    "postinstall": "npm install --force --ignore-scripts --package-lock-only --silent"
  },
  "peerDependencies": {
    "hono": "^4.11.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@prettier/plugin-oxc": "^0.1.3",
    "@socketsecurity/bun-security-scanner": "^1.1.2",
    "@types/bun": "^1.3.5",
    "@typescript/native-preview": "^7.0.0-dev.20260107.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-perfectionist": "^5.3.0",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-tsdoc": "^0.5.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "tsdown": "^0.19.0-beta.5",
    "typedoc": "^0.28.15",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-theme-fresh": "^0.2.3",
    "typescript-eslint": "^8.52.0"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.55.1"
  },
  "trustedDependencies": [
    "esbuild",
    "unrs-resolver"
  ]
}
