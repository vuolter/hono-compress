{
  "name": "hono-compress",
  "type": "module",
  "version": "0.9.0",
  "description": "Compression plugin for Hono working with Bun",
  "author": {
    "name": "Walter Purcaro",
    "url": "https://github.com/vuolter"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuolter/hono-compress"
  },
  "keywords": [
    "hono",
    "hono compress",
    "bun compress",
    "compression",
    "compress",
    "node",
    "deno",
    "bun",
    "edge",
    "zstd",
    "brotli",
    "gzip",
    "deflate"
  ],
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist"
  ],
  "url": "https://github.com/vuolter/hono-compress",
  "scripts": {
    "build": "bun typecheck && pkgroll --clean-dist --minify",
    "format": "prettier . --cache --write",
    "lint": "eslint --cache --cache-location node_modules/.cache/eslint --cache-strategy content --concurrency auto --fix .",
    "typecheck": "tsgo",
    "gendocs": "typedoc && prettier ./docs --cache --write --log-level silent",
    "postinstall": "npm install --force --ignore-scripts --package-lock-only --silent"
  },
  "peerDependencies": {
    "hono": "^4.10.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@socketsecurity/bun-security-scanner": "^1.1.1",
    "@types/bun": "^1.3.1",
    "@typescript/native-preview": "^7.0.0-dev.20251029.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-markdown": "^5.1.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.4.0",
    "pkgroll": "^2.20.1",
    "prettier": "^4.0.0-alpha.12",
    "typedoc": "^0.28.14",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-theme-fresh": "^0.2.1",
    "typescript-eslint": "^8.46.2"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.52.5"
  },
  "trustedDependencies": [
    "esbuild",
    "unrs-resolver"
  ]
}
