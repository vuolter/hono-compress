{
  "entryPoints": ["./src/index.ts"],
  "sort": ["kind", "instance-first"],
  "router": "kind",
  "cleanOutputDir": true,
  "plugin": ["typedoc-plugin-markdown", "typedoc-theme-fresh"],
  "outputs": [
    {
      "name": "html",
      "path": "./docs",
      "options": {
        "out": "docs",
        "hideGenerator": true,
        "includeHierarchySummary": false,
        "searchInComments": true,
        "searchInDocuments": true,
        "theme": "fresh"
      }
    },
    {
      "name": "markdown",
      "path": "./wiki",
      "options": {
        "classPropertiesFormat": "table",
        "enumMembersFormat": "table",
        "formatWithPrettier": true,
        "hidePageHeader": true,
        "indexFormat": "table",
        "interfacePropertiesFormat": "table",
        "mergeReadme": true,
        "out": "wiki",
        "parametersFormat": "table",
        "propertyMembersFormat": "table",
        "publicPath": "https://github.com/vuolter/hono-compress/wiki",
        "theme": "markdown",
        "typeAliasPropertiesFormat": "table",
        "typeDeclarationFormat": "table",
        "useCodeBlocks": true
      }
    }
  ]
}
